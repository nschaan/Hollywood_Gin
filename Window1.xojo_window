		Sub finalGame1()
		  dim a, b as integer 
		  dim final1 as Double
		  dim score1, uu as string
		  
		  a = usScore1.Ubound
		  b = themScore1.Ubound
		  
		  if usScore1(a) > themScore1(b) then
		    final1 = (150 + usScore1(a) - themScore1(b) + ((a-b) * 25))/100
		    if themScore1(b) = 0 then
		      final1 = final1 * 2
		    end if
		    uu = "<--"
		    win1 = "U"
		    dollar1 = final1
		  else
		    final1 = (150 + themScore1(b) - usScore1(a) + ((b - a) * 25))/100.0
		    if usScore1(a) = 0 then
		      final1 = final1 * 2
		    end if
		    win1 = "T"
		    dollar1 = final1
		    uu = "-->"
		  end if
		  
		  score1 = str(final1, "0.00")
		  
		  game1.text = "$" + score1 + uu
		  
		End Sub
