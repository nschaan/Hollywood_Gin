		  scoreIt.Visible = False
		  themCush.Visible = False
		  themScore.Visible = False
		  usCush.Visible = False
		  usScore.Visible = False
		  label3.Visible = False
		  
		  
		  finalSummary.Visible = True
		  
		  order = win1 + win2 + win3
		  
		  Select Case order
		  Case "UUU"
		    woodWinner.Text = "US"
		    totalWood = dollar1 + dollar2 + dollar3
		    woodDollars.text = "$" + str(round(totalWood),"0.00")
		  Case "UUT"
		    totalWood = dollar1 + dollar2 - dollar3
		    if totalWood < 0 Then
		      totalWood = totalWood * -1
		      woodWinner.Text = "Them"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    else
		      woodWinner.Text = "Us"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    end if
		  Case "UTT"
		    totalWood = dollar1 - dollar2 - dollar3
		    if totalWood < 0 Then
		      totalWood = totalWood * -1
		      woodWinner.Text = "Them"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    else
		      woodWinner.Text = "Us"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    end if
		  Case "UTU"
		    totalWood = dollar1 - dollar2 + dollar3
		    if totalWood < 0 Then
		      totalWood = totalWood * -1
		      woodWinner.Text = "Them"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    else
		      woodWinner.Text = "Us"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    end if
		  Case "TTT"
		    totalWood = dollar1 + dollar2 + dollar3
		    woodWinner.Text = "Them"
		    woodDollars.text = "$" + str(round(totalWood),"0.00")
		  Case "TTU"
		    totalWood = dollar1 + dollar2 - dollar3
		    if totalWood < 0 Then
		      totalWood = totalWood * -1
		      woodWinner.Text = "Us"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    else
		      woodWinner.Text = "Them"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    end if
		  Case "TUU"
		    totalWood = dollar1 - dollar2 - dollar3
		    if totalWood < 0 Then
		      totalWood = totalWood * -1
		      woodWinner.Text = "Us"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    else
		      woodWinner.Text = "Them"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    end if
		  Case "TUT"
		    totalWood = dollar1 - dollar2 + dollar3
		    if totalWood < 0 Then
		      totalWood = totalWood * -1
		      woodWinner.Text = "Us"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    else
		      woodWinner.Text = "Them"
		      woodDollars.text = "$" + str(round(totalWood),"0.00")
		    end if
		  Else
		    MsgBox "OTHER"
		  end select
		End Sub
